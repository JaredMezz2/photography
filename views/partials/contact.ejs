<div class="row" id="contact">
    <%- include('../partials/flash')%>
    <div class="container">
        <div class="row">
            <div class="col-md-12 col-lg-3"></div>
            <div class="col-md-12 col-lg-6 contact-section">

                <h2>Setup a Shoot</h2>
                <form action="/contact" name="submit" method="POST" class="needs-validation" novalidate>
                    <input type="hidden" name="recaptchaToken" id="recaptchaToken">
                    <div class="form-group">
                        <label class="form-label" for="name">Name: </label>
                        <input type="text" id="name" name="name" placeholder="John" class="form-control" required>
                    </div>
                    <div class="form-group">
                        <label class="form-label" for="email">Email: </label>
                        <input type="email" id="email" name="email" placeholder="me@google.com" class="form-control" required>
                    </div>
                    <div class="form-group">
                        <label class="form-label" for="message">Message: </label>
                        <textarea name="message" id="message" cols="30" rows="10" class="form-control" required></textarea>
                    </div>
                    <div class="d-grid gap-2 pt-2">
                        <button type="submit" class="btn btn-block btn-outline-dark" disabled>Closed For The Season</button>
                    </div>

                </form>
            </div>
            <div class="col-md-12 col-lg-3"></div>
        </div>
    </div>
</div>

<!--<script src="https://www.google.com/recaptcha/api.js?render=6LeIxAcTAAAAAJcZVRqyHh71UMIEGNQ_MXjiZKhI"></script>-->
<script src="https://www.google.com/recaptcha/api.js?render=6LeqJfEfAAAAAA2CUDsHd3noXjKZIQwdvXIeKnij"></script>
<script>
    grecaptcha.ready(function() {
        grecaptcha.execute('6LeqJfEfAAAAAA2CUDsHd3noXjKZIQwdvXIeKnij', {action: 'submit'})
        // grecaptcha.execute('6LeqJfEfAAAAAA2CUDsHd3noXjKZIQwdvXIeKnij', {action: 'demo'})
            .then(function(token) {
                $('#recaptchaToken').val(token);
                // console.log('token set');
            });
    });
</script>